{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "WORDPRESS_APP_PASSWORD": "@wordpress-app-password",
    "WORDPRESS_USERNAME": "@wordpress-username",
    "MUX_TOKEN_ID": "@mux-token-id",
    "MUX_SECRET": "@mux-secret"
  }
}